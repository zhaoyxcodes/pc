<view class="info" style="width:100%;height: {{max_height}}px;">
  <view style="position:relative;background-color:#4D8AD7;color:#fff;height:90px">
      <view style="position:absolute;bottom:10px;align-items:center;display: flex;height:50px">
        <span style="margin-left:13px;">
          <image style="width:50px;height:50px;border-radius:20%;border:2px solid #6495ED;" mode="aspectFit" src="{{userimg}}"></image>
         
        </span>
        <span style="margin-left:10px;font-weight:bold;font-size:16px;">
          <view wx:if="{{carauth==1}}"> <text class="fa fa-address-card-o" style="font-size:15px;color:#fff;"></text></view>
          <view>{{username}}</view>
        </span>
        
      </view>
  </view>






  <view class="weui-cells weui-cells_after-title" style="margin-top:10px;">
            <navigator url="lift/lift" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd" style="width:30px;">
                   <text class="fa fa-street-view" aria-hidden="true" style="color:#4D8AD7;font-size:16px;" ></text>
                </view>
                <view class="weui-cell__bd" >出行</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
            <navigator url="service/service" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd" style="width:30px;">
                   
                   <text class="fa fa-car" aria-hidden="true" style="color:#4D8AD7;font-size:16px;" ></text>
                </view>
                <view class="weui-cell__bd">服务</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
        </view>





  <view class="weui-cells weui-cells_after-title" style="margin-top:10px;">
            <navigator url="certification/certification" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd" style="width:30px;">
                   <text class="fa fa-taxi" aria-hidden="true" style="color:#f0c14b;font-size:16px;" ></text>
                </view>
                <view class="weui-cell__bd">车主</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
       </view>





  <view class="weui-cells weui-cells_after-title" style="margin-top:10px;">
            <navigator url="money/money" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd" style="width:30px;">
                    <text class="fa fa-money" aria-hidden="true" style="color:#ff8547;font-size:16px;" ></text>
                </view>
                <view class="weui-cell__bd">余额</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
            <navigator url="card/card" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd" style="width:30px;">
                    <text class="fa fa-credit-card-alt" style="color:#ffac38;font-size:16px;" aria-hidden="true"></text>
                </view>
                <view class="weui-cell__bd">卡卷</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
        </view>

        <view class="weui-cells weui-cells_after-title" style="margin-top:10px;" wx:if="{{isadm==1}}">
            <navigator url="audit/audit" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">车主审核</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
            
        </view>


</view>